{
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": [
      "DOM",
      "esnext"
    ],
    "module": "ESNext",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "strictNullChecks": false,
    "target": "ES2017",
    "declaration": false,
    "types": [
      "./types"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "exclude": [
    "**/node_modules",
    "**/dist"
  ],
  "include": [
    "packages/atomic-ui-js-docs/next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ]
}
