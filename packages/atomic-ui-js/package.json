{
  "name": "atomic-ui-js",
  "version": "1.0.0",
  "description": "An atomic user interface toolkit, and library, for building html user interfaces.",
  "module": "index.js",
  "type": "module",
  "typings": "dist/typings/index.d.ts",
  "engines": {
    "node": ">=v20"
  },
  "exports": {
    ".": "./index.js",
    "./*": "./*"
  },
  "scripts": {
    "build": "pnpm build:css",
    "build:css": "pnpm gen:css && node node_scripts/build/index.mjs",
    "watch": "node node_scripts/watch/index.mjs",
    "docs": "pnpm typedoc",
    "gen:css": "node node_scripts/gen-css/index.mjs",
    "eslint": "eslint .",
    "eslintfix": "pnpm eslint --fix",
    "stylelint": "stylelint \"**/*.css\"",
    "stylelintfix": "pnpm stylelint --fix",
    "typedoc": "typedoc .",
    "test": "vitest --run",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o ../../dist/atomic-ui-js/storybook"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 5 chrome version",
      "last 5 firefox version",
      "last 5 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elycruz/atomic-ui-js.git"
  },
  "keywords": [],
  "author": "",
  "license": "LGPL-Clause-3",
  "bugs": {
    "url": "https://github.com/elycruz/atomic-ui-js/issues"
  },
  "homepage": "https://github.com/elycruz/atomic-ui-js#readme",
  "devDependencies": {
    "@cucumber/cucumber": "^10.9.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-links": "^10.1.11",
    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/web-components-vite": "^10.1.11",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/node": "^24.10.9",
    "@types/postcss-import": "^14.0.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/browser-playwright": "^4.0.17",
    "@vitest/coverage-istanbul": "^4.0.17",
    "chokidar": "^3.6.0",
    "cssnano": "^6.1.2",
    "eslint": "^9.39.2",
    "happy-dom": "^20.3.1",
    "lit": "^3.3.2",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^8.5.1",
    "prettier": "^2.8.8",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "rollup": "4.22.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sass": "^1.15.3",
    "sass": "^1.97.2",
    "storybook": "^10.1.11",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "typedoc": "^0.24.8",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17",
    "web-component-analyzer": "^1.1.7"
  },
  "peerDependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "lit": "^3.1.0",
    "tailwindcss": "^4.1.12"
  }
}
