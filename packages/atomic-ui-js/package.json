{
  "name": "atomic-ui-js",
  "version": "1.0.0",
  "description": "An atomic user interface toolkit, and library, for building html user interfaces.",
  "module": "index.js",
  "type": "module",
  "typings": "types",
  "engines": {
    "node": ">=v20"
  },
  "exports": {
    ".": "./index.js",
    "./*": "./*",
    "./*.css": "./dist/*.css"
  },
  "scripts": {
    "build": "pnpm build:css",
    "build:css": "pnpm gen:css && node node_scripts/build/index.mjs",
    "lint": "pnpm eslint && pnpm stylelint",
    "lintfix": "pnpm stylelint --fix && pnpm eslint --fix",
    "watch": "node node_scripts/watch/index.mjs",
    "docs": "pnpm typedoc",
    "gen:css": "node node_scripts/gen-css/index.mjs",
    "eslint": "eslint -c ./.eslintrc.json .",
    "stylelint": "stylelint **/*.{css,scss}",
    "typedoc": "typedoc .",
    "test": "echo \"tests not set up yet\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "browserslist": [
    "defaults",
    "> 0.2% and not dead"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elycruz/atomic-ui-js.git"
  },
  "keywords": [],
  "author": "",
  "license": "LGPL-Clause-3",
  "bugs": {
    "url": "https://github.com/elycruz/atomic-ui-js/issues"
  },
  "homepage": "https://github.com/elycruz/atomic-ui-js#readme",
  "devDependencies": {
    "@cucumber/cucumber": "^12.4.0",
    "@open-wc/testing": "^4.0.0",
    "@rollup/plugin-typescript": "^12.3.0",
    "@storybook/addon-docs": "^10.1.10",
    "@storybook/addon-links": "^10.1.10",
    "@storybook/web-components-vite": "^10.1.10",
    "@types/node": "25.0.3",
    "@web/test-runner": "^0.20.2",
    "@web/test-runner-mocha": "^0.9.0",
    "@web/test-runner-puppeteer": "^0.18.0",
    "chokidar": "^5.0.0",
    "cssnano": "^7.1.2",
    "eslint": "^8.57.1",
    "lit": "^3.3.1",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-preset-env": "^10.5.0",
    "prettier": "^3.7.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "rollup": "4.53.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sass": "^1.15.3",
    "sass": "^1.97.0",
    "storybook": "^10.1.10",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3",
    "web-component-analyzer": "^2.0.0"
  },
  "peerDependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "lit": "^3.1.0",
    "tailwindcss": "^4.1.12"
  }
}
